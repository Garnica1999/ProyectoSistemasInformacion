<% if teacher_signed_in? %>
	<% if current_teacher.especialidad == "Administrador" %>
	<div id="wb_LayoutGridTituloAcademic">
        <div id="LayoutGridTituloAcademic">
            <div class="row">
                <div class="col-1">
                    <div id="wb_TextTituloAcademic">
                        <span style="color:#696969;font-family:'Segoe UI';font-size:32px;"><strong>SISTEMA ACADEMIC</strong></span>
                    </div>
                </div>
            </div>
        </div>
    </div>
	<div id="wb_LayoutGrid1">
	    <div id="LayoutGrid1">
	        <div class="row">
	            <div class="col-1">
	            	<hr id="Line2NewEst">
	                <div id="wb_ShapePanel1">
	                    <a href=<%= "/academics/groups/"%>><div id="ShapePanel1"><div id="ShapePanel1_text"><span style="color:#FFFFFF;font-family:'Segoe UI';font-size:29px;"><strong>Gestionar Grupos</strong></span></div></div></a>
	                </div>
					<hr id="Line4NewEst">
	                <div id="wb_ShapePanel1">
	                    <a href=<%= "/academics/subjects/"%>>
	                    	<div id="ShapePanel1">
	                    		<div id="ShapePanel1_text">
	                    			<span style="color:#FFFFFF;font-family:'Segoe UI';font-size:29px;">
	                    				<strong>Gestionar Materias</strong>
	                    			</span>
	                    		</div>
	                    	</div>
	                	</a>
	                </div>
	            </div>
	            <div class="col-2">
	            	<hr id="Line3NewEst">
	                <div id="wb_ShapePanel2">
	                    <a href=<%= "academics/students" %>><div id="ShapePanel2"><div id="ShapePanel2_text"><span style="color:#FFFFFF;font-family:'Segoe UI';font-size:29px;"><strong>Gestionar Horarios</strong></span></div></div></a>
	                </div>
	            </div>
	            <div class="col-3">
	            	<hr id="Line4NewEst">
	                <div id="wb_ShapePanel3">
	                    <div id="ShapePanel3"><div id="ShapePanel3_text"><span style="color:#FFFFFF;font-family:'Segoe UI';font-size:29px;"><strong>Gestionar Salones</strong></span></div></div>
	                </div>
	            </div>
	            <div class="col-4">
	            	<hr id="Line1NewEst">
	                <div id="wb_ShapePanel4">
	                    <a href=<%= "/academics/students" %>><div id="ShapePanel4"><div id="ShapePanel4_text"><span style="color:#FFFFFF;font-family:'Segoe UI';font-size:29px;"><strong>Gestionar Matriculas</strong></span></div></div></a>
	                </div>
	            </div>
	        </div>
	    </div>
	</div>
    <div id="wb_LayoutGridContentNotificaciones">
        <div id="LayoutGridContentNotificaciones">
            <div class="row">
                <div class="col-1">
                    <div id="wb_TextTituloNotificaciones">
                        <span style="color:#696969;font-family:'Segoe UI';font-size:29px;">NOTIFICACIONES</span>
                    </div>
                    <table id="TableNotificaciones">
                        <tr>
                            <td class="cell0"><span style="color:#696969;font-family:'Segoe UI';font-size:24px;line-height:32px;"><strong>Tipo</strong></span></td>
                            <td class="cell0"><span style="color:#696969;font-family:'Segoe UI';font-size:24px;line-height:32px;"><strong>Accion</strong></span></td>
                            <td class="cell0"><span style="color:#696969;font-family:'Segoe UI';font-size:24px;line-height:32px;"><strong>Provocado por</strong></span></td>
                            <td class="cell0"><span style="color:#696969;font-family:'Segoe UI';font-size:24px;line-height:32px;"><strong>URL</strong></span></td>
                            <td class="cell0"><span style="color:#696969;font-family:'Segoe UI';font-size:24px;line-height:32px;"><strong>Marcar</strong></span></td>
                        </tr>
                        <% @notifications.each do |notificacion| %>
                        	<tr>
	                            <td class="cell0"><span style="color:#696969;font-family:'Segoe UI';font-size:24px;line-height:32px;"><strong><%= notificacion.tipo %></strong></span></td>
	                            <td class="cell0"><span style="color:#696969;font-family:'Segoe UI';font-size:24px;line-height:32px;"><strong><%= notificacion.accion %></strong></span></td>
	                            <td class="cell0"><span style="color:#696969;font-family:'Segoe UI';font-size:24px;line-height:32px;"><strong><%= Student.obtenerNombre(notificacion.student_id.to_i) %></strong></span></td>
	                            <td class="cell0"><span style="color:#696969;font-family:'Segoe UI';font-size:24px;line-height:32px;"><strong><%= notificacion.url %></strong></span></td>
	                            <td class="cell0"><span style="color:#696969;font-family:'Segoe UI';font-size:24px;line-height:32px;"><strong>Marcar</strong></span></td>
	                        </tr>
                        <% end %>
                    </table>
                </div>
            </div>
        </div>
    </div>
	<% end %>
<%else%>
	<h1>Acceso Denegado, no tines permiso para entrar a esta pagina</h1>
	<p>Si tienes algun problema en entrar a esta pagina, contactate con el soporte de la Universidad</p>
<% end %>